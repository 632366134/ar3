/**app.wxss**/
.container-x {
  height: 100%;
}
.container {
  height: 100%;
}
.textline {
  margin-top: 88rpx;
  width: 100%;
  height: 84rpx;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
}
.textline .welcome {
  font-size: 84rpx;
  color: #FFFFFF;
  line-height: 48rpx;
  background: linear-gradient(180deg, #3FBBFE 0%, #A541FF 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.tutu {
  width: 100%;
  height: 46rpx;
  margin-top: 20rpx;
}
.tutuBox {
  width: 100%;
  height: 364rpx;
  display: flex;
}
.tutuBox .tutuImg {
  margin-left: 30rpx;
  width: 275rpx;
  height: 364rpx;
}
.tutuBox .tutudes {
  font-size: 32rpx;
  font-weight: 400;
  color: #FFFFFF;
  margin-top: 110rpx;
  display: flex;
  flex-direction: column;
  margin-left: 40rpx;
}
.tutuBox .tutudes .goARkit {
  margin-top: 46rpx;
  font-weight: 400;
  font-size: 32rpx;
  width: 220rpx;
  height: 80rpx;
  padding: 0 !important;
  border-radius: 40rpx;
  box-sizing: border-box;
}
.case {
  width: 100%;
  height: 46rpx;
  margin-top: 20rpx;
}
.search_enter {
  width: 100%;
}
.modelBtn {
  margin-right: 32rpx;
  font-size: 32rpx;
  background-color: #8cb7fd;
  width: 200rpx;
  height: 68rpx;
  text-align: center;
  line-height: 68rpx;
  border-radius: 20rpx;
  color: #ffffff;
}
.scroll-view {
  height: 322rpx;
  width: 100vw;
  white-space: nowrap;
  display: flex;
  margin-top: 20rpx;
}
.scroll-view .scroll-item {
  margin-left: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 258rpx;
  height: 100%;
  background: radial-gradient(rgba(0, 0, 0, 0), rgba(255, 255, 255, 0.1));
  border: solid 2rpx rgba(255, 255, 255, 0.2);
  border-radius: 50rpx 10rpx 50rpx 10rpx;
  filter: drop-shadow(0 0 15rpx rgba(255, 255, 255, 0.1));
  box-sizing: border-box;
  padding: 15rpx;
}
.scroll-view .scroll-item .text {
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #020202;
  line-height: 48rpx;
  margin-top: 39rpx;
}
.scroll-view .scroll-item .caseBgc {
  width: 230rpx;
  height: 296rpx;
  border-radius: 50rpx 10rpx 50rpx 10rpx;
  position: relative;
}
.scroll-view .scroll-item .caseBgc .scrollImage {
  border-radius: 50rpx 10rpx 50rpx 10rpx;
}
.scroll-view .scroll-item .caseBgc .case_text {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 64rpx   ;
  background: linear-gradient(38deg, rgba(200, 65, 245, 0.5), rgba(79, 57, 222, 0.5));
  z-index: 3;
  border-radius: 50rpx 0rpx 50rpx 10rpx;
  font-size: 32rpx;
  font-family: YouSheBiaoTiHei;
  font-weight: 400;
  color: #FFFFFF;
  text-align: center;
  line-height: 70rpx;
}
.collect {
  width: 100%;
  height: 46rpx;
  margin-top: 30rpx;
}
.collectBox {
  width: 100%;
  height: 356rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.collectBox .collectBgc {
  width: 534rpx;
  height: 356rpx;
}
.collectBox .addCollect {
  width: 100%;
  height: 100%;
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.collectBox .addCollect .imagebox {
  height: 44rpx;
  width: 44rpx;
  margin-bottom: 20rpx;
}
.collectBox .addCollect .text2 {
  font-size: 22rpx;
  color: #FFFFFF;
}
.compListNav {
  width: 100vw;
  margin-top: 50rpx;
  height: 84rpx;
  position: relative;
}
.compListNav .moreComp {
  right: 30rpx;
  bottom: 0;
  position: absolute;
  width: auto;
  display: flex;
  align-items: center;
}
.compListNav .moreComp .moreCompImg {
  margin-right: -20rpx;
  display: flex;
  height: 60rpx;
  width: 60rpx;
  box-sizing: border-box;
  background: #817E93;
  border: 2px solid #FFFFFF;
  border-radius: 50%;
}
.compListNav .moreComp .moreBtn {
  height: 82rpx;
  width: 82rpx;
  margin-left: -10rpx;
}
.compList {
  box-sizing: border-box;
  padding-left: 30rpx;
  display: flex;
  margin-top: 20rpx;
  height: 252rpx;
  width: 100vw;
  white-space: nowrap;
}
.compList .compBorder {
  width: 210rpx;
  height: 210rpx;
  margin-right: 20rpx;
  background: rgba(129, 126, 147, 0.5);
  border-radius: 30rpx;
  padding: 15rpx;
  box-sizing: border-box;
}
.compList .compBorder .compBox {
  width: 180rpx;
  height: 180rpx;
  border-radius: 30rpx;
}
.compList .compBorder .compBox .compLogo {
  border-radius: 30rpx;
  width: 100%;
  height: 100%;
}
.compList .compBorder .compName {
  display: block;
  text-align: center;
  margin-top: 20rpx;
  font-size: 30rpx;
  color: #FFFFFF;
  overflow: hidden;
  text-overflow: ellipsis;
}
