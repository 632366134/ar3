/* componments/tabBar/tabBar.wxss */
.tabBar {
  width: 100%;
  height: 98rpx;
  background: #ffffff;
  box-shadow: 0px -5rpx 5rpx 0px rgba(0, 0, 0, 0.03);
  position: fixed;
  bottom: 0;
  right: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #757474;
}
.tabBar-x {
  width: 100%;
  height: 166rpx;
  background: #ffffff;
  box-shadow: 0px -5rpx 5rpx 0px rgba(0, 0, 0, 0.03);
  position: fixed;
  padding-bottom: 68rpx;
  right: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #757474;
  box-sizing: border-box;
}
.tabBar-box {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tabBar-box .tabBar-image {
  margin-bottom: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
