/* packageA/pages/copmList/copmList.wxss */
.tabs {
  background: #2E2B3F;
  margin-top: 188rpx;
}
.tabs .custom-class {
  width: 100vw !important;
  background: #2E2B3F;
  height: 100%;
}
.tabs .tab-class {
  border: none;
  color: #B2ACD4;
  font-size: 34rpx;
  font-family: YouSheBiaoTiHei;
  font-weight: 400;
  background: #2E2B3F;
}
.tabs .tab-active-class {
  color: #FFFFFF;
}
.tabs .nav-class {
  background: #2E2B3F;
  position: fixed !important;
  width: 100vw;
}
.tabs .nav-class .van-tabs__line {
  z-index: 999;
  box-shadow: 0rpx -5rpx 23rpx 4rpx rgba(146, 102, 250, 0.6) !important;
}
.tabs .wrap-class .van-tabs__scroll {
  background-color: #2E2B3F;
}
.tabs .goTop {
  width: 100%;
  height: 174rpx;
  bottom: 0;
  position: fixed;
}
.tabs .goTop-x {
  width: 100%;
  height: 174rpx;
  bottom: 0;
  position: fixed;
  padding-bottom: 56rpx;
}
